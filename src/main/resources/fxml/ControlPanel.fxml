<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<fx:root type="BorderPane" xmlns="http://javafx.com/javafx/8.0.162-ea"
  xmlns:fx="http://javafx.com/fxml/1">
  <top>
    <Accordion fx:id="accordion" minHeight="50.0" minWidth="50.0" BorderPane.alignment="CENTER">
      <BorderPane.margin>
        <Insets bottom="50.0"/>
      </BorderPane.margin>
    </Accordion>
  </top>
  <bottom>
    <VBox spacing="10.0" BorderPane.alignment="CENTER">
      <children>
        <Button fx:id="goButton" maxHeight="1.7976931348623157E308"
          maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onGo" text="Go!"/>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
          mnemonicParsing="false" onAction="#onClearPath" text="Clear Path"/>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
          mnemonicParsing="false" onAction="#onClearWalls" text="Clear Walls"/>
      </children>
    </VBox>
  </bottom>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
  </padding>
</fx:root>
